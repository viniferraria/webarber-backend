{
  "name": "webarber-backend",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "first": "yarn install",
    "dev": "nodemon ./src/server.js --ignore __tests__",
    "db": "yarn sequelize db:migrate",
    "undo": "yarn sequelize db:migrate:undo:all",
    "pretest": "yarn sequelize db:migrate",
    "test": "jest --runInBand --detectOpenHandles --debug",
    "posttest": "yarn sequelize db:migrate:undo:all",
    "prestart": "yarn sequelize db:migrate",
    "start": "node ./src/server.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
